{
  "name": "ts-react-template",
  "version": "1.0.0",
  "description": "This is a template for quickly prototyping React apps with Typescript.",
  "main": "index.tsx",
  "scripts": {
    "build": "rm -rf dist && webpack --config webpack/webpack.prod.js --mode production",
    "dev": "webpack serve --open --config webpack/webpack.dev.js && yarn server",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "server": "node server/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlemast/ts-react-template.git"
  },
  "keywords": [
    "react",
    "typescript",
    "template"
  ],
  "author": "Nathan Le Master",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nlemast/ts-react-template/issues"
  },
  "homepage": "https://github.com/nlemast/ts-react-template#readme",
  "dependencies": {
    "express": "^4.17.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "^5.26.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
